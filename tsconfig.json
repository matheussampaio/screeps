{
  "compilerOptions": {
    "baseUrl": "src/",
    "experimentalDecorators": true,
    "lib": [
      "esnext"
    ],
    "module": "esnext",
    "moduleResolution": "Node",
    "noImplicitReturns": true,
    "outDir": "dist",
    "resolveJsonModule": true,
    "sourceMap": false,
    "suppressExcessPropertyErrors": true,
    "target": "es2017"
  }
}
