    "lodash": "4.17.19",
    "nodemon": "1.19.4",
    "prettier": "1.19.1",
    "rollup": "1.27.0",
    "rollup-plugin-clear": "2.0.7",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-typescript2": "0.25.2",
    "tslint": "5.20.1",
    "tslint-config-prettier": "1.18.0",
